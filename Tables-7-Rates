CREATE TABLE rate_batting (
player_id VARCHAR(9),
player_name VARCHAR(45),
team_id VARCHAR(3),
season_id SMALLINT,
age SMALLINT,
bat CHAR(1),
b_avg DECIMAL(4,3),
b_obp DECIMAL (4,3),
b_slg DECIMAL (4,3),
b_ops DECIMAL (4,3),
b_iso DECIMAL (4,3),
b_bb_pct DECIMAL (3,2),
b_so_pct DECIMAL (3,2),
PRIMARY KEY (player_id,team_id,season_id)
);
