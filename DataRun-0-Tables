CREATE TABLE lines_player_bio (
player_id VARCHAR(9),
last_name VARCHAR(20),
first_name VARCHAR(20),
full_name VARCHAR(40),
birth_date INT,
birth_city VARCHAR(20),
birth_state VARCHAR(20),
birth_country VARCHAR(20),
